  "$id": "https://ns.adobe.com/xdm/mixins/profile-computed-attributes",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "XDM Profile Computed Attributes",
  "type": "object",
  "meta:extensible": true,
  "meta:abstract": true,
  "meta:service": true,
  "meta:intendedToExtend": ["https://ns.adobe.com/xdm/context/profile"],
  "description": "This fieldgroup is used to add computed attributes by internal Adobe services to incoming customer data. This should not be used by customers to ingest data.",
  "definitions": {
    "computed-attributes": {
      "properties": {
        "xdm:computedAttributes": {
          "type": "object",
          "properties": {
            "xdm:datasetID": {
              "title": "Dataset ID",
              "description": "Dataset ID.",
              "type": "string"
            },
            "xdm:fragmentID": {
              "title": "Fragment ID",
              "description": "Fragment ID.",
              "type": "string"
            },
            "xdm:lastUpdatedAt": {
              "title": "Last Updated At",
              "description": "Last updated at..",
              "type": "string",
              "format": "date-time"
            },
            "xdm:window": {
              "title": "Source External ID",
              "description": "External source unique identifier at the source.",
              "type": "object",
              "properties": {
                "xdm:start": {
                  "title": "Start Date Time",
                  "description": "Start date time.",
                  "type": "string",
                  "format": "date-time"
                },
                "xdm:end": {
                  "title": "End Date Time",
                  "description": "End date time.",
                  "type": "string",
                  "format": "date-time"
                }
              }
            }
          }
        }
      }
    }
  },
  "allOf": [
    {
      "$ref": "#/definitions/computed-attributes"
    }
  ],
  "meta:status": "experimental"
}
