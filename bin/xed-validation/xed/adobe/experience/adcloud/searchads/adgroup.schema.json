                        "Active",
                        "Paused",
                        "Deleted",
                        "Ended",
                        "Pending",
                        "Inactive",
                        "Disapproved",
                        "Incomplete",
                        "Offline",
                        "On Hold (Editing)",
                        "On Hold (Other)",
                        "Duplicate",
                        "Orphan",
                        "Optimize",
                        "Disabled",
                        "Unknown",
                        "Dummy"
