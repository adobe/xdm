{
    "$id": "adcloud/searchads/account.schema.json",
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "Search Advertising Account",
    "type": "object",
    "meta:extensible": [
        false,
        true
    ],
    "meta:abstract": [
        false,
        true
    ],
    "meta:auditable": true,
    "description": "Search Advertising Account defines Customer Configuration on Search Advertising Platform.",
    "meta:status": "experimental",
    "meta:xdmType": "object",
    "properties": {
        "_id": {
            "title": "Identifier",
            "type": "string",
            "format": "uri-reference",
            "description": "A unique identifier for the record.",
            "meta:xdmType": "string",
            "meta:xdmField": "@id",
            "tagNumber": 1
        },
        "repositoryCreatedBy": {
            "title": "Created by user identifier",
            "type": "string",
            "description": "User ID of who created the record.",
            "meta:xdmType": "string",
            "meta:xdmField": "xdm:repositoryCreatedBy",
            "tagNumber": 2
        },
        "repositoryLastModifiedBy": {
            "title": "Modified by user identifier",
            "type": "string",
            "description": "User ID of who last modified the record. At creation time, `modifiedByUser` is set as `createdByUser`.",
            "meta:xdmType": "string",
            "meta:xdmField": "xdm:repositoryLastModifiedBy",
            "tagNumber": 3
        },
        "createdByBatchID": {
            "title": "Created by batch identifier",
            "type": "string",
            "format": "uri-reference",
            "description": "The dataset files in Catalog which has been originating the creation of the record.",
            "meta:xdmType": "string",
            "meta:xdmField": "xdm:createdByBatchID",
            "tagNumber": 4
        },
        "modifiedByBatchID": {
            "title": "Modified by batch identifier",
            "type": "string",
            "format": "uri-reference",
            "description": "The last dataset files in Catalog which has modified the record. At creation time, `modifiedByBatchID` is set as `createdByBatchID`.",
            "meta:xdmType": "string",
            "meta:xdmField": "xdm:modifiedByBatchID",
            "tagNumber": 5
        },
        "_repo": {
            "type": "object",
            "meta:xdmType": "object",
            "properties": {
                "createDate": {
                    "type": "string",
                    "format": "date-time",
                    "meta:immutable": true,
                    "meta:usereditable": false,
                    "examples": [
                        "2004-10-23T12:00:00-06:00"
                    ],
                    "description": "The server date and time when the resource was created in the repository, such as when an asset file is first uploaded or a directory is created by the server as the parent of a new asset. The date time property should conform to ISO 8601 standard. An example form is \"2004-10-23T12:00:00-06:00\".",
                    "meta:xdmType": "date-time",
                    "meta:xdmField": "repo:createDate",
                    "tagNumber": 1
                },
                "modifyDate": {
                    "type": "string",
                    "format": "date-time",
                    "meta:usereditable": false,
                    "examples": [
                        "2004-10-23T12:00:00-06:00"
                    ],
                    "description": "The server date and time when the resource was last modified in the repository, such as when a new version of an asset is uploaded or a directory's child resource is added or removed. The date time property should conform to ISO 8601 standard. An example form is \"2004-10-23T12:00:00-06:00\".",
                    "meta:xdmType": "date-time",
                    "meta:xdmField": "repo:modifyDate",
                    "tagNumber": 2
                }
            },
            "tagNumber": 6
        },
        "_searchads": {
            "type": "object",
            "meta:xdmType": "object",
            "properties": {
                "accountId": {
                    "title": "Account Identifier",
                    "type": "string",
                    "description": "Identifier that defines Customer/Client ID setup on the Search Advertising Platform.",
                    "meta:xdmType": "string",
                    "meta:xdmField": "searchads:accountId",
                    "tagNumber": 1
                },
                "accountName": {
                    "title": "Account Name",
                    "type": "string",
                    "description": "Name of the Account.",
                    "meta:xdmType": "string",
                    "meta:xdmField": "searchads:accountName",
                    "tagNumber": 2
                },
                "currencyCode": {
                    "title": "Currency Code",
                    "type": "string",
                    "examples": [
                        "USD",
                        "EUR",
                        "JPY"
                    ],
                    "pattern": "^[A-Z]{3}$",
                    "description": "Currency in which this account operates.",
                    "meta:xdmType": "string",
                    "meta:xdmField": "searchads:currencyCode",
                    "tagNumber": 3
                },
                "accountStatus": {
                    "title": "Account Status",
                    "type": "string",
                    "description": "Managed Flag indicates whether account is enabled or disabled on search networks.",
                    "enum": [
                        "Enabled",
                        "Disabled"
                    ],
                    "meta:xdmType": "string",
                    "meta:xdmField": "searchads:accountStatus",
                    "tagNumber": 4
                },
                "budget": {
                    "title": "Budget",
                    "type": "number",
                    "description": "Budget Allocated to the Account acts only as a spend cap across all your campaigns.",
                    "meta:xdmType": "number",
                    "meta:xdmField": "searchads:budget",
                    "tagNumber": 5
                },
                "budgetType": {
                    "title": "Budget Type",
                    "type": "string",
                    "description": "Budget Type",
                    "enum": [
                        "Daily",
                        "Monthly",
                        "Lifetime",
                        "Daily Spend Until Depleted",
                        "Day of Week"
                    ],
                    "meta:xdmType": "string",
                    "meta:xdmField": "searchads:budgetType",
                    "tagNumber": 6
                },
                "searchEngineId": {
                    "title": "Search Engine Identifier",
                    "type": "integer",
                    "description": "The applicatio-specified identifier used to identify the Search Advertising Platform Name.",
                    "meta:xdmType": "int",
                    "meta:xdmField": "searchads:searchEngineId",
                    "tagNumber": 7
                },
                "createDate": {
                    "type": "string",
                    "format": "date-time",
                    "examples": [
                        "2004-10-23T12:00:00-06:00"
                    ],
                    "description": "The date and time when the account was created. An example form is \"2004-10-23T12:00:00-06:00\".",
                    "meta:xdmType": "date-time",
                    "meta:xdmField": "searchads:createDate",
                    "tagNumber": 8
                },
                "modifyDate": {
                    "type": "string",
                    "format": "date-time",
                    "examples": [
                        "2004-10-23T12:00:00-06:00"
                    ],
                    "description": "The date and time when the account was last modified. An example form is \"2004-10-23T12:00:00-06:00\".",
                    "meta:xdmType": "date-time",
                    "meta:xdmField": "searchads:modifyDate",
                    "tagNumber": 9
                }
            },
            "tagNumber": 7
        }
    },
    "meta:xdmId": "https://ns.adobe.com/xdm/adcloud/searchads/account",
    "meta:altId": "_xdm.adcloud.searchads.account"
}