    "title": "Adobe Advertising Cloud Ad Details",
    "type": "object",
    "description": "Adobe Advertising Cloud Ad Details.",
    "definitions": {
        "advertisement": {
            "properties": {
                "xdm:adId": {
                    "title": "Ad Identifier",
                    "type": "string",
                    "description": "Identifier for the ad this event is associated with. Not related to the Ad-ID standard.",
                    "meta:titleId": "advertisement##xdm:adId##title##33851",
                    "meta:descriptionId": "advertisement##xdm:adId##description##35091"
                },
                "xdm:runtime": {
                    "title": "Runtime Type",
                    "type": "string",
                    "description": "Runtime of the ad unit itself. Not the runtime of the browser or the operating system.",
                    "meta:enum": {
                        "unknown": "Unknown",
                        "HTML5": "HTML5"
                    },
                    "meta:titleId": "advertisement##xdm:runtime##title##81141",
                    "meta:descriptionId": "advertisement##xdm:runtime##description##78311"
                },
                "xdm:adClass": {
                    "title": "Ad Class",
                    "type": "string",
                    "description": "Ad class of driving event.",
                    "meta:enum": {
                        "display": "Display",
                        "video": "Video",
                        "social": "Social"
                    },
                    "meta:titleId": "advertisement##xdm:adClass##title##37911",
                    "meta:descriptionId": "advertisement##xdm:adClass##description##1171"
                },
                "xdm:adUnitType": {
                    "title": "Ad Unit Type",
                    "type": "string",
                    "description": "Identifier for the literal piece of code driving display of the ad in a browser/device.",
                    "meta:enum": {
                        "linearVideo": "Linear video",
                        "interactiveVideo": "Interactive video",
                        "banner": "Banner",
                        "richMediaBanner": "Rich media banner",
                        "newsFeedVideo": "News feed video",
                        "newsFeedDisplay": "News feed display",
                        "HTML5": "HTML5",
                        "inPageVideo": "In page video",
                        "inPageDisplay": "In page display",
                        "facebook": "Facebook",
                        "twitter": "Twitter",
                        "linearTv": "Linear TV",
                        "vod": "Video on Demand"
                    },
                    "meta:titleId": "advertisement##xdm:adUnitType##title##65021",
                    "meta:descriptionId": "advertisement##xdm:adUnitType##description##44401"
                },
                "xdm:promotedAssetId": {
                    "title": "Promoted Asset Identifier",
                    "type": "string",
                    "description": "Identifier for the underlying asset being promoted in the ad which this event is associated with.",
                    "meta:titleId": "advertisement##xdm:promotedAssetId##title##63891",
                    "meta:descriptionId": "advertisement##xdm:promotedAssetId##description##73871"
                },
                "xdm:creativeID": {
                    "title": "Creative Identifier",
                    "type": "string",
                    "description": "Identifier for the creative which this event is associated with.",
                    "meta:titleId": "advertisement##xdm:creativeID##title##31601",
                    "meta:descriptionId": "advertisement##xdm:creativeID##description##9901"
                },
                "xdm:keywordID": {
                    "title": "Keyword Identifier",
                    "type": "string",
                    "description": "Identifier for the keyword that the surfer used to search, which triggered this click event.",
                    "meta:titleId": "advertisement##xdm:keywordID##title##7221",
                    "meta:descriptionId": "advertisement##xdm:keywordID##description##78561"
                },
                "xdm:keyword": {
                    "title": "Keyword",
                    "type": "string",
                    "description": "Listing keyword for which customer is bidding ",
                    "meta:titleId": "advertisement##xdm:keyword##title##38981",
                    "meta:descriptionId": "advertisement##xdm:keyword##description##54891"
                },
                "xdm:isDynamicSearchAd": {
                    "title": "Is Dynamic Search Advertisement",
                    "type": "boolean",
                    "description": "Indicates if the click comes from a dynamic search advertisement.",
                    "meta:titleId": "advertisement##xdm:isDynamicSearchAd##title##73271",
                    "meta:descriptionId": "advertisement##xdm:isDynamicSearchAd##description##57191"
                },
                "xdm:audienceID": {
                    "title": "Audience Identifier",
                    "type": "string",
                    "description": "Identifier for the audience segment for which the ad is targeted.",
                    "meta:titleId": "advertisement##xdm:audienceID##title##94221",
                    "meta:descriptionId": "advertisement##xdm:audienceID##description##39021"
                },
                "xdm:adGroupID": {
                    "title": "Ad Group Identifier",
                    "type": "string",
                    "description": "Identifier for the adGroup which ad is displayed and triggered this click event.",
                    "meta:titleId": "advertisement##xdm:adGroupID##title##87951",
                    "meta:descriptionId": "advertisement##xdm:adGroupID##description##29141"
                },
                "xdm:campaignID": {
                    "title": "Campaign Identifier",
                    "type": "string",
                    "description": "Identifier for the campaign that belongs to the current displayed advertisement which triggers this click event.",
                    "meta:titleId": "advertisement##xdm:campaignID##title##74831",
                    "meta:descriptionId": "advertisement##xdm:campaignID##description##82711"
                },
                "xdm:networkType": {
                    "title": "Network Type",
                    "type": "string",
                    "description": "Network type the click event came from a search advertisement or content advertisement.",
                    "meta:enum": {
                        "search": "Advertisement is displayed in Search Network",
                        "content": "Advertisement is displayed in Content Network"
                    },
                    "meta:titleId": "advertisement##xdm:networkType##title##65261",
                    "meta:descriptionId": "advertisement##xdm:networkType##description##21951"
                },
                "xdm:matchType": {
                    "title": "matchType",
                    "type": "string",
                    "description": "Match type used to match keyword.",
                    "meta:enum": {
                        "exact": "Exact Match of the keyword",
                        "broad": "Broad Match of the keyword",
                        "phrase": "Phrase Match of the keyword"
                    },
                    "meta:titleId": "advertisement##xdm:matchType##title##86241",
                    "meta:descriptionId": "advertisement##xdm:matchType##description##64851"
                }
            }
        }
    },
    "allOf": [
        {
            "$ref": "#/definitions/advertisement"
        }
