                            "meta:xdmField": "https://ns.adobe.com/experience/target/mboxname",
                            "tagNumber": 2
                        },
                        "mboxversion": {
                            "title": "Adobe Target Mbox Version",
                            "type": "string",
                            "description": "Adobe Target mbox version.",
