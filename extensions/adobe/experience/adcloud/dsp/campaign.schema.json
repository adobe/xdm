          "meta:titleId": "campaign##dsp:feeClass##title",
          "meta:descriptionId": "campaign##dsp:feeClass##description"
        },
        "dsp:campaignStartTime": {
          "title": "Campaign start time.",
          "type": "string",
          "format": "date-time",
          "description": "The insertion order fields representing campaign start time.",
          "meta:titleId": "campaign##dsp:campaignStartTime##title",
          "meta:descriptionId": "campaign##dsp:campaignStartTime##description"
