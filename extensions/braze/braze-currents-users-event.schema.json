  "$id": "https://ns.braze.com/currents/users-event",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Braze Currents Users Event",
  "description": "Braze Currents users event payload",
  "type": "object",
  "meta:extensible": true,
  "meta:intendedToExtend": [
    "https://ns.adobe.com/xdm/context/experienceevent"
  ],
  "definitions": {
    "currents-users-event": {
      "properties": {
        "braze:appID": {
          "title": "App ID",
          "type": "string",
          "description": "API ID of the app on which this event occurred."
        },
        "braze:timezone": {
          "title": "Timezone",
          "type": "string",
          "description": "Time zone of the user."
        },
        "braze:appGroupID": {
          "title": "App Group ID",
          "type": "string",
          "description": "API ID of the app group this user belongs to."
        },
        "braze:source": {
          "title": "Source",
          "type": "string",
          "description": "The source of the attribution."
        },
        "braze:user": {
          "title": "User",
          "type": "object",
          "properties": {
            "braze:userID": {
              "title": "User ID",
              "type": "string",
              "description": "Braze user ID of the user who performed this event."
            },
            "braze:externalUserID": {
              "title": "External User ID",
              "type": "string",
              "description": "[PII] External ID of the user."
            },
            "braze:emailAddress": {
              "title": "Email Address",
              "type": "string",
              "description": "[PII] Email address of the user."
            },
            "braze:language": {
              "title": "Language",
              "type": "string",
              "description": "[PII] Language of the user."
            },
            "braze:country": {
              "title": "Country",
              "type": "string",
              "description": "[PII] Country of the user."
            },
            "braze:gender": {
              "title": "Gender",
              "type": "string",
              "description": "[PII] Gender of the user.",
              "enum": [
                "F",
                "M",
                "N",
                "O",
                "P"
              ],
              "meta:enum": {
                "F": "Female",
                "M": "Male",
                "N": "Not applicable",
                "O": "Other",
                "P": "Prefer not to say"
              }
            },
            "braze:phoneNumber": {
              "title": "Phone Number",
              "type": "string",
              "description": "[PII] Phone number of the user in e.164 format (for example +14155552671)."
            }
          }
        },
        "braze:device": {
          "title": "Device",
          "type": "object",
          "properties": {
            "braze:platform": {
              "title": "Platform",
              "type": "string",
              "description": "Platform of the device."
            },
            "braze:OSVersion": {
              "title": "OS Version",
              "type": "string",
              "description": "Version of the operating system of the device."
            },
            "braze:deviceModel": {
              "title": "Device Model",
              "type": "string",
              "description": "Model of the device."
            },
            "braze:deviceClass": {
              "title": "Device Class",
              "type": "string",
              "description": "Device class extracted from user_agent (mobile, desktop, tablet, etc...)."
            },
            "braze:deviceOS": {
              "title": "Device OS",
              "type": "string",
              "description": "Device operating system extracted from user_agent."
            },
            "braze:deviceID": {
              "title": "Device ID",
              "type": "string",
              "description": "ID of the device on which the event occurred."
            },
            "braze:SDKVersion": {
              "title": "SDK Version",
              "type": "string",
              "description": "Version of the Braze SDK in use during the event."
            },
            "braze:browser": {
              "title": "Browser",
              "type": "string",
              "description": "Device browser - extracted from user_agent - on which the open occurred."
            },
            "braze:carrier": {
              "title": "Carrier",
              "type": "string",
              "description": "Carrier of the device."
            },
            "braze:resolution": {
              "title": "Resolution",
              "type": "string",
              "description": "Resolution of the device."
            }
          }
        },
        "braze:customEvent": {
          "title": "Custom Event",
          "type": "object",
          "properties": {
            "braze:name": {
              "title": "Name",
              "type": "string",
              "description": "Name of the custom event."
            },
            "braze:properties": {
              "title": "Properties",
              "type": "string",
              "description": "Custom properties stored as a JSON encoded string."
            }
          }
        },
        "braze:campaign": {
          "title": "Campaign",
          "type": "object",
          "properties": {
            "braze:campaignID": {
              "title": "Campaign ID",
              "type": "string",
              "description": "API ID of the campaign this event belongs to."
            },
            "braze:campaignName": {
              "title": "Campaign Name",
              "type": "string",
              "description": "Name of the campaign."
            },
            "braze:messageVariationID": {
              "title": "Message Variation ID",
              "type": "string",
              "description": "API ID of the message variation this user received."
            },
            "braze:messageVariationName": {
              "title": "Message Variation Name",
              "type": "string",
              "description": "Name of the message variation."
            }
          }
        },
        "braze:canvas": {
          "title": "Canvas",
          "type": "object",
          "properties": {
            "braze:canvasEntryID": {
              "title": "Canvas Entry ID",
              "type": "string",
              "description": "Unique identifier for this instance of a user in a canvas."
            },
            "braze:canvasID": {
              "title": "Canvas ID",
              "type": "string",
              "description": "API ID of the Canvas this event belongs to."
            },
            "braze:canvasStepMessageVariationID": {
              "title": "Canvas Step Message Variation ID",
              "type": "string",
              "description": "API ID of the Canvas step message variation this user received."
            },
            "braze:canvasName": {
              "title": "Canvas Name",
              "type": "string",
              "description": "Name of the Canvas."
            },
            "braze:canvasVariationID": {
              "title": "Canvas Variation ID",
              "type": "string",
              "description": "API ID of the Canvas variation this event belongs to."
            },
            "braze:canvasVariationName": {
              "title": "Canvas Variation Name",
              "type": "string",
              "description": "Name of the Canvas variation this user received."
            },
            "braze:canvasStepID": {
              "title": "Canvas Step ID",
              "type": "string",
              "description": "API ID of the Canvas step this event belongs to."
            },
            "braze:canvasStepName": {
              "title": "Canvas Step Name",
              "type": "string",
              "description": "Name of the Canvas step."
            },
            "braze:exitReason": {
              "title": "Exit Reason",
              "type": "string",
              "description": "If this is an exit, the reason a user exited the canvas during the step."
            },
            "braze:isCanvasEntry": {
              "title": "Is Canvas Entry",
              "type": "boolean",
              "description": "Whether this is entry into a first step in a canvas."
            },
            "braze:nextStepID": {
              "title": "Next Step ID",
              "type": "string",
              "description": "API ID of the next step in the canvas."
            },
            "braze:progressionType": {
              "title": "Progression Type",
              "type": "string",
              "description": "What type of step progression event this is."
            },
            "braze:experiment": {
              "title": "Experiment",
              "type": "object",
              "properties": {
                "braze:experimentStepID": {
                  "title": "Experiment Step ID",
                  "type": "string",
                  "description": "API ID of the experiment step this event belongs to."
                },
                "braze:experimentSplitID": {
                  "title": "Experiment Split ID",
                  "type": "string",
                  "description": "API ID of the experiment split the user enrolled in."
                },
                "braze:experimentSplitName": {
                  "title": "Experiment Split Name",
                  "type": "string",
                  "description": "Name of the experiment split."
                },
                "braze:inControlGroup": {
                  "title": "In Control Group",
                  "type": "boolean",
                  "description": "Whether the user was enrolled in the control group."
                }
              }
            }
          }
        },
        "braze:featureflag": {
          "title": "FeatureFlag",
          "type": "object",
          "properties": {
            "braze:featureFlagIDName": {
              "title": "Feature Flag ID Name",
              "type": "string",
              "description": "The Feature Flag Rollout identifier."
            }
          }
        },
        "braze:messaging": {
          "title": "Messaging",
          "type": "object",
          "properties": {
            "braze:sendID": {
              "title": "Send ID",
              "type": "string",
              "description": "Message send ID this message belongs to (SMS Sends events only)."
            },
            "braze:dispatchID": {
              "title": "Dispatch ID",
              "type": "string",
              "description": "ID of the dispatch this message belongs to."
            },
            "braze:messageBody": {
              "title": "Message Body",
              "type": "string",
              "description": "Typed response from the user."
            },
            "braze:messageExtras": {
              "title": "Message Extras",
              "type": "string",
              "description": "[PII] A JSON string of the tagged key-value pairs during liquid rendering."
            },
            "braze:mediaUrls": {
              "title": "Media Urls",
              "type": "array",
              "description": "Media URLs from the user.",
              "items": {
                "type": "string"
              }
            },
            "braze:action": {
              "title": "Action",
              "type": "string",
              "description": "Action taken in response to this message. (for example Subscribed, Unsubscribed or None).",
              "enum": [
                "Subscribed",
                "Unsubscribed",
                "None",
                "Help",
                "Pending Double Opt-In"
              ],
              "meta:enum": {
                "Subscribed": "Subscribed",
                "Unsubscribed": "Unsubscribed",
                "None": "None",
                "Help": "Help",
                "Pending Double Opt-In": "Pending Double Opt-In"
              }
            },
            "braze:abortLog": {
              "title": "Abort Log",
              "type": "string",
              "description": "[PII] Log message describing abort details (up to 128 chars)."
            },
            "braze:abortType": {
              "title": "Abort Type",
              "type": "string",
              "description": "Type of abort, one of ['liquid_abort_message', 'message_ttl', 'quiet_hours', 'template_parse_error'].",
              "enum": [
                "liquid_abort_message",
                "quiet_hours",
                "sms_no_sending_numbers",
                "users_cti_marked_as_deleted",
                "blocked_recipient_country",
                "exhausted_cc_retries",
                "whats_app_fatal_provider_error",
                "template_parse_error"
              ],
              "meta:enum": {
                "liquid_abort_message": "Liquid Abort Message",
                "quiet_hours": "Quiet Hours",
                "sms_no_sending_numbers": "SMS No Sending Numbers",
                "users_cti_marked_as_deleted": "Users CTI Marked as Deleted",
                "blocked_recipient_country": "Blocked Recipient Country",
                "exhausted_cc_retries": "Exhausted CC Retries",
                "whats_app_fatal_provider_error": "WhatsApp Fatal Provider Error",
                "template_parse_error": "Template Parse Error"
              }
            },
            "braze:cardID": {
              "title": "Card ID",
              "type": "string",
              "description": "API ID of the card."
            },
            "braze:contentCardID": {
              "title": "Content Card ID",
              "type": "string",
              "description": "ID of the card that generated this event."
            },
            "braze:phoneNumber": {
              "title": "Phone Number",
              "type": "object",
              "properties": {
                "braze:fromPhoneNumber": {
                  "title": "From Phone Number",
                  "type": "string",
                  "description": "Phone number used to send in e.164 format (for example +14155552671)."
                },
                "braze:toPhoneNumber": {
                  "title": "To Phone Number",
                  "type": "string",
                  "description": "[PII] Phone number of the user receiving the message in e.164 format (for example +14155552671)."
                },
                "braze:inboundPhoneNumber": {
                  "title": "Inbound Phone Number",
                  "type": "string",
                  "description": "The inbound number that the message was sent to."
                },
                "braze:userPhoneNumber": {
                  "title": "User Phone Number",
                  "type": "string",
                  "description": "[PII] The user's phone number from which the message was received."
                }
              }
            },
            "braze:email": {
              "title": "Email",
              "type": "object",
              "properties": {
                "braze:sendingIP": {
                  "title": "Sending IP",
                  "type": "string",
                  "description": "IP address from which the email send was made."
                },
                "braze:IPPool": {
                  "title": "IP Pool",
                  "type": "string",
                  "description": "IP pool from which the email send was made."
                },
                "braze:ESP": {
                  "title": "ESP",
                  "type": "string",
                  "description": "ESP related to the event (Sparkpost or Sendgrid).",
                  "enum": [
                    "SparkPost",
                    "SendGrid"
                  ],
                  "meta:enum": {
                    "SparkPost": "SparkPost",
                    "SendGrid": "SendGrid"
                  }
                },
                "braze:fromDomain": {
                  "title": "From Domain",
                  "type": "string",
                  "description": "Sending domain for the email."
                },
                "braze:userAgent": {
                  "title": "User Agent",
                  "type": "string",
                  "description": "User agent on which the spam report occurred."
                },
                "braze:machineOpen": {
                  "title": "Machine Open",
                  "type": "string",
                  "description": "Populated to 'true' if the open event is triggered without user engagement, for example by an Apple device with Mail Privacy Protection enabled. Value may change over time to provide more granularity.",
                  "enum": [
                    "true"
                  ],
                  "meta:enum": {
                    "true": "true"
                  }
                },
                "braze:isAMP": {
                  "title": "Is AMP",
                  "type": "boolean",
                  "description": "Indicates that this is an AMP event."
                },
                "braze:URL": {
                  "title": "URL",
                  "type": "string",
                  "description": "URL that the user clicked on."
                },
                "braze:linkID": {
                  "title": "Link ID",
                  "type": "string",
                  "description": "Unique ID for the link which was clicked, as created by Braze."
                },
                "braze:linkAlias": {
                  "title": "Link Alias",
                  "type": "string",
                  "description": "Alias associated with this link ID."
                },
                "braze:bounceReason": {
                  "title": "Bounce Reason",
                  "type": "string",
                  "description": "[PII] The SMTP reason code and user friendly message received for this bounce event."
                },
                "braze:isDrop": {
                  "title": "Is Drop",
                  "type": "boolean",
                  "description": "Indicates that this event counts as a drop event."
                },
                "braze:mailboxProvider": {
                  "title": "Mailbox Provider",
                  "type": "string",
                  "description": "Mailbox provider value returned by the ESP for this event."
                }
              }
            },
            "braze:sms": {
              "title": "SMS",
              "type": "object",
              "properties": {
                "braze:category": {
                  "title": "Category",
                  "type": "string",
                  "description": "Keyword category name, only populated for auto-reply messages: 'opt-in', 'opt-out', 'help', or custom value."
                },
                "braze:shortURL": {
                  "title": "Short URL",
                  "type": "string",
                  "description": "Shortened URL that was clicked."
                }
              }
            },
            "braze:whatsapp": {
              "title": "WhatsApp",
              "type": "object",
              "properties": {
                "braze:quickReplyText": {
                  "title": "Quick Reply Text",
                  "type": "string",
                  "description": "Text of button pressed by the user."
                }
              }
            },
            "braze:pushNotification": {
              "title": "Push Notification",
              "type": "object",
              "properties": {
                "braze:buttonString": {
                  "title": "Button String",
                  "type": "string",
                  "description": "Identifier (button_string) of the push notification button clicked. null if not from a button click."
                },
                "braze:buttonActionType": {
                  "title": "Button Action Type",
                  "type": "string",
                  "description": "Action type of the push notification button, null if not from a button click. One of ['uri', 'deep_link', 'none', 'close'].",
                  "enum": [
                    "URI",
                    "DEEP_LINK",
                    "NONE",
                    "CLOSE"
                  ],
                  "meta:enum": {
                    "URI": "URI",
                    "DEEP_LINK": "Deep Link",
                    "NONE": "None",
                    "CLOSE": "Close"
                  }
                }
              }
            },
            "braze:in-appMessage": {
              "title": "In-App Message",
              "type": "object",
              "properties": {
                "braze:version": {
                  "title": "Version",
                  "type": "string",
                  "description": "Which version of in-app message, legacy or triggered."
                },
                "braze:buttonID": {
                  "title": "Button ID",
                  "type": "string",
                  "description": "ID of the button clicked, if this click represents a click on a button."
                }
              }
            },
            "braze:error": {
              "title": "Error",
              "type": "object",
              "properties": {
                "braze:error": {
                  "title": "Error",
                  "type": "string",
                  "description": "Error name."
                },
                "braze:providerErrorCode": {
                  "title": "Provider Error Code",
                  "type": "string",
                  "description": "Error code from WhatsApp."
                },
                "braze:providerErrorTitle": {
                  "title": "Provider Error Title",
                  "type": "string",
                  "description": "Description of error from WhatsApp."
                }
              }
            }
          }
        },
        "braze:session": {
          "title": "Session",
          "type": "object",
          "properties": {
            "braze:duration": {
              "title": "Duration",
              "type": "number",
              "description": "Duration of the session in seconds."
            },
            "braze:sessionID": {
              "title": "Session ID",
              "type": "string",
              "description": "UUID of the session."
            }
          }
        },
        "braze:adTracking": {
          "title": "Ad Tracking",
          "type": "object",
          "properties": {
            "braze:adID": {
              "title": "Ad ID",
              "type": "string",
              "description": "[PII] Advertising identifier."
            },
            "braze:adIDType": {
              "title": "Ad ID Type",
              "type": "string",
              "description": "One of ['ios_idfa', 'google_ad_id', 'windows_ad_id', 'roku_ad_id'].",
              "enum": [
                "ios_idfa",
                "google_ad_id",
                "windows_ad_id",
                "roku_ad_id"
              ],
              "meta:enum": {
                "ios_idfa": "iOS ID for advertisers",
                "google_ad_id": "Google Ad ID",
                "windows_ad_id": "Windows Advertiser ID",
                "roku_ad_id": "Roku Ad ID"
              }
            },
            "braze:adTrackingEnabled": {
              "title": "Ad Tracking Enabled",
              "type": "boolean",
              "description": "Whether advertising tracking is enabled for the device."
            }
          }
        },
        "braze:purchase": {
          "title": "Purchase",
          "type": "object",
          "properties": {
            "braze:productID": {
              "title": "Product ID",
              "type": "string",
              "description": "ID of the product purchased."
            },
            "braze:price": {
              "title": "Price",
              "type": "number",
              "description": "Price of the purchase."
            },
            "braze:currency": {
              "title": "Currency",
              "type": "string",
              "description": "Currency of the purchase."
            }
          }
        },
        "braze:subscription": {
          "title": "Subscription",
          "type": "object",
          "properties": {
            "braze:subscriptionGroupID": {
              "title": "Subscription Group ID",
              "type": "string",
              "description": "Subscription group API ID."
            },
            "braze:subscriptionStatus": {
              "title": "Subscription Status",
              "type": "string",
              "description": "Subscription status: 'Subscribed' or 'Unsubscribed'.",
              "enum": [
                "Subscribed",
                "Unsubscribed",
                "Pending Double Opt-In",
                "Opted In"
              ],
              "meta:enum": {
                "Subscribed": "Subscribed",
                "Unsubscribed": "Unsubscribed",
                "Pending Double Opt-In": "Pending Double Opt-In",
                "Opted In": "Opted In"
              }
            },
            "braze:channel": {
              "title": "Channel",
              "type": "string",
              "description": "Channel this event belongs to."
            },
            "braze:stateChangeSource": {
              "title": "State Change Source",
              "type": "string",
              "description": "Source of the state change, e.g: REST API, SDK, Dashboard, etc."
            }
          }
        },
        "braze:location": {
          "title": "Location",
          "type": "object",
          "properties": {
            "braze:latitude": {
              "title": "Latitude",
              "type": "number",
              "description": "[PII] Latitude of recorded location."
            },
            "braze:longitude": {
              "title": "Longitude",
              "type": "number",
              "description": "[PII] Longitude of recorded location."
            },
            "braze:altitude": {
              "title": "Altitude",
              "type": "number",
              "description": "[PII] Altitude of recorded location."
            },
            "braze:llAccuracy": {
              "title": "Ll Accuracy",
              "type": "number",
              "description": "Accuracy of the latitude and longitude of recorded location."
            },
            "braze:altAccuracy": {
              "title": "Alt Accuracy",
              "type": "number",
              "description": "Altitude accuracy of recorded location."
            }
          }
        },
        "braze:conversion": {
          "title": "Conversion",
          "type": "object",
          "properties": {
            "braze:conversionBehavior": {
              "title": "Conversion Behavior",
              "type": "string",
              "description": "JSON-encoded string describing the conversion behavior."
            },
            "braze:conversionBehaviorIndex": {
              "title": "Conversion Behavior Index",
              "type": "integer",
              "description": "Index of the conversion behavior."
            }
          }
        }
      }
    }
  },
  "allOf": [
    {
      "$ref": "#/definitions/currents-users-event"
    }
  ],
  "meta:status": "experimental"
}
