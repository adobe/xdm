    "$id": "https://ns.adobe.com/experience/campaign/orchestration/reportingeventmetrics",
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "User journey reporting event metrics",
    "type": "object",
    "description": "For state transitions, we capture certain metrics along with the dimensions. This schema hold the metrics associated with reporting of state transition",
    "definitions": {
        "reportingeventmetrics": {
            "properties": {
                "https://ns.adobe.com/experience/campaign/orchestrationEnteredJourneyInstance": {
                    "title": "Entered Journey",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when a journey instance is created",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationEnteredJourneyInstance##title##73631",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationEnteredJourneyInstance##description##2381"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationExitedJourneyInstance": {
                    "title": "Exited journey",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when a journey instance is flagged as ended",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExitedJourneyInstance##title##38391",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExitedJourneyInstance##description##12341"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationErrorInJourneyInstance": {
                    "title": "Error in Journey Instance",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when an error is raised in a journey instance",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationErrorInJourneyInstance##title##1441",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationErrorInJourneyInstance##description##85461"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationErrorInEventProcessing": {
                    "title": "Eroor in step processing",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when an error occurs during an event processing, that is during a step processing",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationErrorInEventProcessing##title##80111",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationErrorInEventProcessing##description##17381"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationErrorInActionExecution": {
                    "title": "Error in action execution",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when an error occurs during an action execution",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationErrorInActionExecution##title##34991",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationErrorInActionExecution##description##33961"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationErrorInJumpActionExecution": {
                    "title": "Error in jump action",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when an error occurs during the jump action execution",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationErrorInJumpActionExecution##title##31591",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationErrorInJumpActionExecution##description##23421"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationErrorInEnrichment": {
                    "title": "Error in enrichment",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when an error occurs during enrichment",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationErrorInEnrichment##title##2741",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationErrorInEnrichment##description##49971"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationExecutedEvent": {
                    "title": "Event processed",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when an event as triggered a transition in a journey instance",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExecutedEvent##title##19041",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExecutedEvent##description##98011"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationExecutedAction": {
                    "title": "Action executed",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when an action is executed successfully in a journey instance",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExecutedAction##title##53311",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExecutedAction##description##86661"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationExecutedFlowControl": {
                    "title": "Flow control executed",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when an action is executed successfully in a journey instance",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExecutedFlowControl##title##51331",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExecutedFlowControl##description##1341"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationExecutedJumpAction": {
                    "title": "Executed jump action",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when a jump action is executed successfully in a journey instance",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExecutedJumpAction##title##3041",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExecutedJumpAction##description##12551"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationExecutedEnrichment": {
                    "title": "Enrichment made successfully ",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when an enrichment is executed successfully in a journey instance",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExecutedEnrichment##title##2001",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationExecutedEnrichment##description##56041"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationJumpToJourney": {
                    "title": "Jump action executed successfully",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when a jump to another journey instance is made",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationJumpToJourney##title##44891",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationJumpToJourney##description##82211"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationTimeoutInAction": {
                    "title": "Timeout In Action",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when timeout occurs during execution of an action",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationTimeoutInAction##title##73391",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationTimeoutInAction##description##97231"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationTimeoutInEvent": {
                    "title": "Timeout in Event",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when timeout occurs during the processing of an event",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationTimeoutInEvent##title##60511",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationTimeoutInEvent##description##76441"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationTimeoutInJourney": {
                    "title": "Timeout In Journey",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when timeout occurs during the flow of a journey",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationTimeoutInJourney##title##95951",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationTimeoutInJourney##description##58431"
                },
                "https://ns.adobe.com/experience/campaign/orchestrationTimeoutInEnrichment": {
                    "title": "Timeout In Enrichment",
                    "$ref": "https://ns.adobe.com/xdm/data/measure",
                    "description": "Set to 1 when timeout occurs during enrichement",
                    "meta:titleId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationTimeoutInEnrichment##title##40531",
                    "meta:descriptionId": "reportingeventmetrics##https://ns.adobe.com/experience/campaign/orchestrationTimeoutInEnrichment##description##78871"
                }
            }
        }
    },
    "allOf": [
        {
            "$ref": "#/definitions/reportingeventmetrics"
        }
    ],
    "meta:status": "stable",
    "meta:titleId": "reportingeventmetrics##title##26401",
    "meta:descriptionId": "reportingeventmetrics##description##81601",
    "examples": [
        {
            "https://ns.adobe.com/experience/campaign/orchestrationEnteredJourneyInstance": {
                "xdm:value": 1
            },
            "https://ns.adobe.com/experience/campaign/orchestrationErrorInJourneyInstance": {
                "xdm:value": 1
            }
        }
    ]
}