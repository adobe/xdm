          "meta:descriptionId": "campaign##dsp:campaignKey##description"
        },
        "dsp:campaignName": {
          "title": "Campaign Name",
          "type": "string",
          "description": "Name of the campaign.",
          "meta:titleId": "campaign##dsp:campaignName##title",
          "meta:descriptionId": "campaign##dsp:campaignName##description"
        },
        "dsp:campaignStatus": {
          "title": "Campaign Status",
          "type": "string",
          "description": "Campaign Status extracted from the Insertion Order associated with this campaign.Indicates if underlying ads are eligible to serve.",
          "enum": [
            "Active",
            "Inactive",
            "Deleted",
            "Paid"
