                            "meta:xdmField": "https://ns.adobe.com/experience/target/clientcode",
                            "tagNumber": 1
                        },
                        "mboxname": {
                            "title": "Adobe Target Mbox Name",
                            "type": "string",
                            "description": "Adobe Target mbox name.",
