            "xdm:stallingImpactedStreams": {
                "xdm:value": 0
            },
            "xdm:bitrateChanges": {
                "xdm:value": 0
            },
            "xdm:bitrateChangeImpactedStreams": {
                "xdm:value": 0
            },
            "xdm:bitrateAverage": {
                "xdm:value": 320
            },
            "xdm:bitrateAverageBucket": "800-899",
            "xdm:droppedFrames": {
